<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Lean Eclipse">
         <property
               name="appName"
               value="Lean Eclipse">
         </property>
         <property
               name="windowImages"
               value="images/eclipse16.png,images/eclipse32.png,images/eclipse48.png">
         </property>
         <property
               name="aboutImage"
               value="images/about.png">
         </property>
         <property
               name="aboutText"
               value="Lean Eclipse for Joro">
         </property>
         <property
               name="preferenceCustomization"
               value="preferences.ini">
         </property>
      </product>
   </extension>

</plugin>
